<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->  
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->  
<!--[if !IE]><!--> <html lang="en"> <!--<![endif]-->  
<head>
    <title>Responsive Resume/CV Template for Developers</title>
    <!-- Meta -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Responsive HTML5 Resume/CV Template for Developers">
    <meta name="author" content="Xiaoying Riley at 3rd Wave Media">    
    <link rel="shortcut icon" href="favicon.ico">  
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,500,400italic,300italic,300,500italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>
    <!-- Global CSS -->
    <link rel="stylesheet" href="/public/stylesheets/template-css/nonresponsive-bootstrap.css">  
    <!-- Plugins CSS -->
    <link rel="stylesheet" href="/public/stylesheets/template-css/font-awesome/css/font-awesome.min.css">      
    <!-- Theme CSS -->  
    <link id="theme-style" rel="stylesheet" href="/public/stylesheets/template-css/styles-<%= resume.templateId %>.css">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head> 

<body>
    <div class="wrapper">
        <div class="sidebar-wrapper">
            <div class="profile-container">
                <% if (resume.photoUrl) { %><img class="profile" src="<%= resume.photoUrl || "images/no-photo.png" %>" alt="Profile picture" /><% } %>
                <h1 class="name"><%= resume.firstName || ""%> <%= resume.lastName || "" %></h1>
                <h3 class="tagline"><%- resume.sumHeadline || ""%></h3>
            </div><!--//profile-container-->
            
            <div class="contact-container container-block">
                <ul class="list-unstyled contact-list">
                    <% if (resume.email) { %><li class="email"><i class="fa fa-envelope"></i><a href="mailto: <%= resume.email %>"><%= resume.email %></a></li> <% } %>
                    <% if (resume.phone) { %><li class="phone"><i class="fa fa-phone"></i><a href="tel:<%= resume.phone %>"><%= resume.phone %></a></li> <% } %>
                    <% if (resume.website) { %><li class="website"><i class="fa fa-globe"></i><a href="<%= resume.website %>" target="_blank"><%= resume.website %></a></li><% } %>
                    <% if (resume.address) { %><li class="address"><i class="fa fa-map-marker"></i><%= resume.address %></li> <% } %>
                </ul>
            </div><!--//contact-container-->
            <% if (resume.educations) { %>
            <div class="education-container container-block">
                <h2 class="container-block-title">Education</h2>
                <% for (var i = 0 ; i < resume.educations.length ; i++) { %>
                <div class="item">
                    <h4 class="degree"><%= resume.educations[i].degree %></h4>
                    <h5 class="meta"><%= resume.educations[i].institute %></h5>
                    <div class="time"><%= resume.educations[i].startMonth %> - <%= resume.educations[i].endMonth %></div>
                    <div class="edu-detail"><%- resume.educations[i].detail %></div>
                </div><!--//item-->
                <% } %>                
            </div><!--//education-container-->
            <% } if (resume.languages) { %>
            <div class="languages-container container-block">
                <h2 class="container-block-title">Languages</h2>
                <ul class="list-unstyled interests-list">
                    <li><%= resume.languages[i].name %> <span class="lang-desc"> <%= resume.languages[i].proficiency %> </span></li>
                </ul>
            </div><!--//languages-->
            <% } %>          
            
        </div><!--//sidebar-wrapper-->
        
        <div class="main-wrapper">
            
            <section class="section summary-section">
                <h2 class="section-title"><i class="fa fa-user"></i>Career Profile</h2>
                <div class="summary">
                    <%- resume.sumContent %>
                </div><!--//summary-->
            </section><!--//section-->
            <% if (resume.experiences) { %>
            <section class="section experiences-section">
                <h2 class="section-title"><i class="fa fa-briefcase"></i>Experiences</h2>                
                <% for (var i=0; i < resume.experiences.length; i++) { %>
                <div class="item">
                    <div class="meta">
                        <div class="upper-row">
                            <h3 class="job-title"><%= resume.experiences[i].designation %></h3>
                            <div class="time"><%= resume.experiences[i].startMonth %> - <%= resume.experiences[i].endtMonth %></div>
                        </div><!--//upper-row-->
                        <div class="company"><%= resume.experiences[i].company %></div>
                    </div><!--//meta-->
                    <div class="details">
                        <%- resume.experiences[i].details %>
                    </div><!--//details-->
                </div><!--//item-->
                <% } %>
            </section><!--//section-->
            <% } %>
            <% if (resume.projects) { %>
            <section class="section projects-section">
                <h2 class="section-title"><i class="fa fa-archive"></i>Projects</h2>
                <% for(var i=0; i<resume.projects.length; i++) {%>
                <div class="item">
                    <span class="project-title">
                        <a href="<%= resume.projects[i].url || "#" %>"><%= resume.projects[i].title %></a>
                    </span>
                    <div class="project-detail"><%- resume.projects[i].detail %></div>
                    <span class="project-tagline">Time: <%= resume.projects[i].startTime.getMonth() %> - <%= resume.projects[i].endTime.getMonth() %></span>                  
                </div><!--//item-->
                <% } %>               
                
            </section><!--//section-->
            <% } %>
            <% if (resume.skills) { %>
            <section class="skills-section section">
                <h2 class="section-title"><i class="fa fa-rocket"></i>Skills &amp; Proficiency</h2>
                <div class="skillset">
                <% for(var i=0; i<resume.skills.length; i++) { %>        
                    <div class="item">
                        
                        <% if (resume.skills[i].name) { %><h3 class="level-title"><%= resume.skills[i].name %></h3><% } %>
                        <span></span>
                        <div class="level-bar">
                            <div class="level-bar-inner" data-level="<%= resume.skills[i].expertise * 20 %>%"><%= resume.skills[i].expertise %>
                            </div>                                                                  
                        </div><!--//level-bar--> 
                        <p class="text-small">
                            <% if (resume.skills[i].experience) { %>Experiences: <%= resume.skills[i].experience %><br /><% } %> 
                            <% if (resume.skills[i].lastUsed) { %>
                                Last used: <%= (resume.skills[i].lastUsed.getMonth() + 1) +"/"+ resume.skills[i].lastUsed.getFullYear() %><% } %>
                        </p>                                
                    </div><!--//item-->   
                <% } %>             
                </div>  
            </section><!--//skills-section-->
        <% } %>    
            
        </div><!--//main-body-->
    </div>
 
    <footer class="footer">
        <div class="text-center">
                <!--/* This template is released under the Creative Commons Attribution 3.0 License. Please keep the attribution link below when using for your own project. Thank you for your support. :) If you'd like to use the template without the attribution, you can check out other license options via our website: themes.3rdwavemedia.com */-->
                <small class="copyright">Designed with <i class="fa fa-heart"></i> by <a href="http://themes.3rdwavemedia.com" target="_blank">Xiaoying Riley</a> for developers</small>
        </div><!--//container-->
    </footer><!--//footer-->
 
    <!-- Javascript -->          
    <script type="text/javascript" src="/public/javascripts/template/jquery-1.11.3.min.js"></script>
    <script type="text/javascript" src="/public/javascripts/template/bootstrap.min.js"></script>    
    <!-- custom js -->
    <script type="text/javascript" src="/public/javascripts/template/main-min.js"></script>            
</body>
</html> 